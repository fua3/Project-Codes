<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	
	 <!-- 配置适配器 -->
	<servlet>
		<servlet-name>ServiceAgent</servlet-name>
		<servlet-class>
			org.bjit.serviceadapter.agent.ServiceAgent
		</servlet-class>
		<init-param>
			<param-name>debug</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<description>mapkey</description>
			<param-name>mapkey</param-name>
			<param-value>maptile,maptilex</param-value>
		</init-param>
		<init-param>
			<description>user</description>
			<param-name>user</param-name>
			<param-value>testV4</param-value>
		</init-param>
		<init-param>
			<description>是否加密</description>
			<param-name>encrypt</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<description>参数编码方式</description>
			<param-name>encode</param-name>
			<param-value>GBK</param-value>
		</init-param>
		<init-param>
			<description>password</description>
			<param-name>password</param-name>
			<param-value>testV4123</param-value>
		</init-param>
	<init-param>
			<description>手机服务</description>
			<param-name>sisserver</param-name>
			<param-value>
			http://172.26.15.45/service/SisService/sisserver
			</param-value>
		</init-param>
		<init-param>
			<description>手机地图服务</description>
			<param-name>maptilex</param-name>
			<param-value>
			http://172.26.15.45/service/SisService/maptilex
			</param-value>
		</init-param>
		<init-param>
			<description>全文检索</description>
			<param-name>WholeSearch</param-name>
			<param-value>
			http://localhost:8080/service/WholeSearch
			</param-value>
		</init-param>
		<init-param>
			<description>手机地图服务</description>
			<param-name>maptile/maptile</param-name>
			<param-value>
			http://172.26.15.45/service/SisService/maptile/maptile
			</param-value>
		</init-param>
		<init-param>
			<description>图片引擎(Vector)</description>
			<param-name>EzServer/EzMap</param-name>
			<param-value>
				http://10.192.26.220:8080/service/ImageEngine/EzServer/EzMap
			</param-value>
		</init-param>
	</servlet>
		<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/sisserver</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/maptilex</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/maptile/maptile</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/WholeSearch</url-pattern>
		</servlet-mapping>
				<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/WholeSearch</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>ServiceAgent</servlet-name>
			<url-pattern>/EzServer/EzMap</url-pattern>
		</servlet-mapping>
</web-app>
